          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ny3MQ5q8EeuPJMavBbeVSA" name="actualizar" specification="_nqJ8MJq8EeuPJMavBbeVSA">
            <language>java</language>
            <body>comprobarCorriendo();&#xD;
