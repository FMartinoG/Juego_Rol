            </ownedParameter>
            <language>java</language>
            <body>String texto = &quot;&quot;;&#xD;
&#xD;
		InputStream is = CargadorRecursos.class.getResourceAsStream(ruta);&#xD;
		BufferedReader br = new BufferedReader(new InputStreamReader(is));&#xD;
		String linea;&#xD;
		try {&#xD;
			while ((linea = br.readLine()) != null) {&#xD;
				texto += linea;&#xD;
			}&#xD;
			br.close();&#xD;
			is.close();&#xD;
		} catch (IOException e) {&#xD;
			e.printStackTrace();&#xD;
		}&#xD;
&#xD;
		return texto;&#xD;
	</body>
