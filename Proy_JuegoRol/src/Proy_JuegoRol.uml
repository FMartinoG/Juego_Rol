              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_nzGc15q8EeuPJMavBbeVSA" name="dibujar" specification="_nxIt8Jq8EeuPJMavBbeVSA">
                <ownedParameter xmi:id="_nzGc2Jq8EeuPJMavBbeVSA" name="g" type="_nvknsZq8EeuPJMavBbeVSA"/>
                <language>java</language>
                <body>mapa.dibujar(g, (int) jugador.getPosicionX(), (int) jugador.getPosicionY());&#xD;
		jugador.dibujar(g);&#xD;
		if (enConversacion) {&#xD;
			mostarMensajes(g);&#xD;
			enConversacion = false;&#xD;
		}&#xD;
	</body>
              </ownedBehavior>
              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_nzGc2Zq8EeuPJMavBbeVSA" name="mostarMensajes" specification="_nxIt9Zq8EeuPJMavBbeVSA">
                <ownedParameter xmi:id="_nzGc2pq8EeuPJMavBbeVSA" name="g" type="_nvknsZq8EeuPJMavBbeVSA"/>
                <language>java</language>
                <body>Mensaje m = null;&#xD;
		for (String s : Constantes.CONVERSACIONES.get(conversacion)) {&#xD;
			m = new Mensaje(s, 200, 300, true);&#xD;
			m.dibujar(g);&#xD;
			long msInicio = System.currentTimeMillis();&#xD;
			long msActual = System.currentTimeMillis();&#xD;
			while ((msActual - msInicio) &lt; 500)&#xD;
				msActual = System.currentTimeMillis();&#xD;
&#xD;
			while (!Controles.TECLADO.seleccion) {&#xD;
				System.out.println(&quot;Enter para continuar&quot;);&#xD;
			}&#xD;
		}&#xD;
	</body>
